package com.pcwk.ehr.ed02;

public class J04Exam04 {
	public static void display(int[] test) {
		for(int test : nums) {
			System.out.print(nums+",");
		}
	}
	
	public static void main(String[] args) {
		int[] num = {7,4,5,1,3};
		display(num);
		for(int i = 0; i < num.length; i++) {
        	for(int k = i+1; k < num.length; k++) {
        		if(num[i] > num[k]) {
        			int tmp = num[i];
        			num[i] = num[k];
        			num[k] = tmp;
        		}
        	}
        }
		display(num);


	}

}
