package com.pcwk.ehr.ed03;

import java.util.Scanner;

public class Ed01DoWhile {

	public static void main(String[] args) {
		//메세지를 입력 받고, 출력, q를 입력하면 프로그램 종료.
		Scanner sc = new Scanner(System.in);
		System.out.println("┌──────────────────────┐");
		System.out.println("│                      │");
		System.out.println("└──────────────────────┘");

	}

}
