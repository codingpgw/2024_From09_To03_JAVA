package com.pcwk.ehr.ed02;

import java.util.Scanner;

public class Ed05While {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = 0;
		int sum = 0;
		System.out.print("7자리의 정수를 입력하시오. > ");
		num = sc.nextInt();
		
		
		while(num > 0) {
			sum = num % 10;
			System.out.println(sum);
		}
		System.out.println(sum);

	}

}
